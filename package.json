{
  "name": "bootstrap-theme-hive13",
  "version": "0.0.1",
  "description": "Bootstrap theme for Hive13.",
  "main": "index.js",
  "scripts": {
    "test": "brunch watch -s",
    "build": "node node_modules/less/bin/lessc --include-path=\"node_modules/bootstrap/less:src\" src/bootstrap-hive13.less dist/bootstrap-hive13.css"
  },
  "keywords": [
    "bootstrap",
    "theme",
    "hive13"
  ],
  "author": "Joseph Sikorski",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.4",
    "less-brunch": "git://github.com/joedski/less-brunch.git#1.8.2",
    "brunch": "~1.7"
  },
  "options": {
    "less": {
      "paths": [
        "node_modules/bootstrap/less",
        "src"
      ]
    }
  },
  "devDependencies": {
    "less": "^2.4.0",
    "less-plugin-clean-css": "^1.5.0"
  }
}
