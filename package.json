{
  "name": "bootstrap-theme-hive13",
  "repository": {
    "type": "git",
    "url": "https://github.com/joedski/bootstrap-theme-hive13.git"
  },
  "version": "0.0.1",
  "description": "Bootstrap theme for Hive13.",
  "main": "index.js",
  "scripts": {
    "test": "brunch watch -s",
    "build": "node node_modules/less/bin/lessc --source-map --include-path=\"bower_components/bootstrap/less:src\" src/bootstrap-hive13.less dist/bootstrap-hive13.css"
  },
  "keywords": [
    "bootstrap",
    "theme",
    "hive13"
  ],
  "author": "Joseph Sikorski",
  "license": "ISC",
  "dependencies": {
    "auto-reload-brunch": "^1.7.8",
    "brunch": "~1.7",
    "handlebars-brunch": "^1.9.1",
    "javascript-brunch": ">= 1.0 < 1.8",
    "less-brunch": "git://github.com/joedski/less-brunch.git#1.8.2"
  },
  "options": {
    "less": {
      "paths": [
        "bower_components/bootstrap/less",
        "src"
      ]
    }
  },
  "devDependencies": {
    "less": "^2.4.0",
    "less-plugin-clean-css": "^1.5.0"
  }
}
